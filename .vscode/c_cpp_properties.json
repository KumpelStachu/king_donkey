{
	"configurations": [
		{
			"name": "macos-clang-arm64",
			"includePath": ["${workspaceFolder}/**", "/opt/homebrew/include/**", "./src"],
			"compilerPath": "/usr/bin/clang",
			"cStandard": "c99",
			"cppStandard": "${default}",
			"intelliSenseMode": "macos-clang-arm64",
			"compilerArgs": [
				"-Wall",
				"-Wextra",
				"-Werror",
				"-pedantic",
				"-Wno-unused-parameter",
				"-Wno-unused-variable",
				"-Wno-format"
			]
		}
	],
	"version": 4
}
